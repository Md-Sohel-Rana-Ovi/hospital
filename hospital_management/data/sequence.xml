<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for patient -->
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for doctor -->
        <record id="seq_hospital_doctor" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
            <field name="code">hospital.doctor.sequence</field>
            <field name="prefix">HD</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

         <!-- Sequences for appointment -->
        <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">appointment Sequence</field>
            <field name="code">hospital.appointment.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

         <!-- Sequences for worker -->
        <record id="seq_hospital_worker" model="ir.sequence">
            <field name="name">worker Sequence</field>
            <field name="code">hospital.worker.sequence</field>
            <field name="prefix">Hw</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>



    </data>
</odoo>
